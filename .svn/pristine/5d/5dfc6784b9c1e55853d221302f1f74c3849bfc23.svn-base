/* 表单----开始 */
/* 表单中label的字体颜色 */
.myStyle .el-form-item__label{color:#474747;}
.homePage .el-form-item__label{padding:0 20px 0 0;}

/* 表单的圆角 */
.myStyle .el-input__inner{border-radius: 0}

/* 行内表单上下、左右距离 */
.myStyle .el-form-item{margin-bottom:30px;}
.baseInfo .myStyle .el-form-item{margin-right:50px;}
.baseInfo .myStyle.shifts .el-input__inner{padding:0 10px;}
.baseInfo .myStyle.shifts .el-form-item{margin-bottom:24px;}
.baseInfo .myStyle.shifts .el-form-item{margin-right:30px;}

/* 表单----结尾 */

/* 表格----开始 */
/* 表格中每一行的高度、字体 原本是距离左边16px,因为div.cell有10px，所以padding要减10*/
.myStyle tbody tr{height:50px;}
.myStyle tbody tr td{font:16px/50px "";padding:0 6px;color:#474747;}

/* .baseInfo .myStyle tbody tr{height:35px;}
.baseInfo .myStyle tbody tr td{font:16px/35px "";padding:0;color:#474747;} */
/* 表格----结尾 */

/* 去掉分页的padding和margin值 */
.myStyle.el-pagination{padding:0}
.myStyle.el-pagination button.btn-next{padding:0;margin-right:0;}